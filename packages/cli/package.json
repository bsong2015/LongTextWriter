{
  "name": "@gendoc/cli",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "gendoc": "dist/index.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rimraf dist public",
    "copy:ui": "copyfiles -u 3 ../web-ui/dist/**/* public",
    "copy:locales": "copyfiles -u 2 ../../locales/**/* dist/locales",
    "build": "npm run clean && npm run copy:ui && npm run copy:locales && cross-env NODE_ENV=production tsc -p .",
    "test": "jest --config ../../jest.config.js",
    "start:web": "ts-node -r tsconfig-paths/register src/server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@inquirer/prompts": "^7.8.2",
    "@langchain/openai": "^0.6.7",
    "archiver": "^7.0.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "langchain": "^0.3.30",
    "lodash": "^4.17.21",
    "marked": "^16.1.2",
    "multer": "^2.0.2",
    "zod": "^3.25.76",
    "@gendoc/shared": "^1.0.0",
    "undici": "^6.19.2",
    "cli-progress": "^3.12.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@types/cli-progress": "^3.11.5",
    "copyfiles": "^2.4.1",
    "jest": "^30.0.5",
    "rimraf": "^5.0.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}